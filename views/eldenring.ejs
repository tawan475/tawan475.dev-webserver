<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script data-ad-client="ca-pub-9231912947022457" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    <link rel="stylesheet" href="/file/html/css/header.css">
    <link rel="stylesheet" href="/file/html/css/animate.min.css">
    <link rel="stylesheet" href="/file/html/css/normalize.css">
    
    <link rel='icon' href='/file/html/favicon/favicon.ico' type='image/x-icon' />
    <title id="siteTitle">tawan475.dev</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
        @font-face {
            font-family: 'EB Garamond';
            font-style: normal;
            font-weight: 400;
            font-display: swap;
            src: url(data:font/woff2;charset=utf-8;base64,) format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
        }
    </style>
    <script>$(document).ready(function(){$("#siteTitle").html(window.location.hostname),$("#siteName").html(window.location.hostname),$.getJSON("/visits",function(a){document.getElementById("counter").innerHTML=`<small>Site visits: ${a.visits}</small>`})});</script>
</head>

<body>
    <nav>
        <div>
            <div class="animated bounceInDown">
                <p class="rainbow rainbow_text_animated" id="siteName"></p>
            </div>
            <table align="center" class="animated bounceInUp">
                <td><a href="/">Index</a></td>
                <td><a href="/file">Files</a></td>
                <td><a href="/upload">Upload</a></td>
                <td><a href="/tools">Tools</a></td>
            </table>
        </div>
    </nav>
	
    <main align="center" class="animated bounceInUp" style="color: white; background-color: #23272a; align-items: center;">
        <div>Set the message before selecting file.</div>
        <div>Right click the image to Save/Copy</div>
        <div>Or share it using Shareable Link.</div>
        </br>
        <div>Message: <input id="messageInput" type='text' value="ENEMY FELLED"/></div>
        <div>File: <input id="fileSelector" type='file'/></div>
        <div id="downloadButtonDiv" style="display: none;"><button type="button" id="downloadButton">Download</button></div>
        <div id="result"></div>
        </br>
        <div><canvas id="canvas"></canvas></div>
        <div style="text-align: center;" id="counter"></div>
        <div><small>if this ever goes down, use <a href="https://enemyfelled.com">enemyfelled.com</a></small></div>
        <div><small>from my friend expand, its basically the same thing</small></div>
    </main>
</body>

<script>
    const ctx = document.getElementById('canvas').getContext('2d');
    const fileInput = document.getElementById('fileSelector');
    const messageInput = document.getElementById('messageInput');
    const downloadButton = document.getElementById("downloadButton");
    const downloadButtonDiv = document.getElementById("downloadButtonDiv");
    
    fileInput.onchange = function(fileEvent){
        let input = fileEvent.target;
        
        if (input.files && input.files[0]) {
            if ((input.files[0].size / (1000 * 1000)) > 50) return document.getElementById("result").innerHTML=`File too big! ${(input.files[0].size / (1000 * 1000))}MB (Limit: 50MB)`
            
            let filename = input.files[0].name;
            document.getElementById("result").innerHTML='';
            downloadButtonDiv.style.display = "none";
            downloadButton.onclick = null;
            
            var reader = new FileReader();
            
            reader.onload = function (readEvent) {
                let file = readEvent.target;
                var img = new Image();
                img.src = file.result;
                img.onload = function(){
                    // set canvas + draw image
                    ctx.canvas.width = this.width
                    ctx.canvas.height = this.height
                    ctx.drawImage(this, 0, 0, this.width, this.height);
                    
                    let size = ((this.width)/(Math.log(messageInput.value.length+1)/Math.log(1.15)));
                    
                    // draw background line
                    // make gradient
                    var grd = ctx.createLinearGradient(0, (this.height/2) - (size), 0, (this.height/2) + (size *0.8));
                    grd.addColorStop(0, 'rgba(0, 0, 0, 0)');
                    grd.addColorStop(0.25, 'rgba(0, 0, 0, 0.5)');
                    grd.addColorStop(0.75, 'rgba(0, 0, 0, 0.5)');
                    grd.addColorStop(1, 'rgba(0, 0, 0, 0)');
                    // draw line
                    ctx.fillStyle = grd;
                    ctx.shadowColor = 'black';
                    ctx.shadowBlur = 5;
                    ctx.fillRect(0, (this.height/2) - size, this.width, size * 2.4);

                    // draw background text
                    ctx.font = (size * 1.1) + 'px EB Garamond';
                    ctx.fillStyle = 'rgba(254, 241, 51, 0.2)';
                    ctx.shadowColor = '';
                    ctx.shadowBlur = 0;
                    ctx.textAlign = "center";
                    ctx.textBaseline = 'middle';
                    ctx.fillText(messageInput.value, this.width/2, this.height/2);
                    
                    // draw text
                    ctx.font = size + 'px EB Garamond';
                    ctx.fillStyle = 'rgba(254, 241, 51, 1)';
                    ctx.shadowColor = 'rgba(254, 241, 51, 1)';
                    ctx.shadowBlur = 5;
                    ctx.textAlign = "center";
                    ctx.textBaseline = 'middle';
                    ctx.fillText(messageInput.value, this.width/2, this.height/2);
                    
                    // draw credit
                    ctx.globalCompositeOperation = "difference";
                    ctx.font = (size/4 > 30 ? 30 : (size/4 > 22 ? 22 : (size/4 > 14 ? 14 : (size/4 > 8 ? size/4 : size/2)))) + 'px EB Garamond';
                    ctx.fillStyle = 'rgba(128,128,128, 0.8)';
                    ctx.shadowColor = 'rgba(0, 0, 0, 0)';
                    ctx.shadowBlur = 0;
                    ctx.textAlign = "end";
                    ctx.textBaseline = 'bottom';
                    ctx.fillText("tawan475.dev/eldenring", this.width, this.height);
                    
                    
                    fetch(document.getElementById('canvas').toDataURL('image/png'))
                        .then(res => res.blob())
                        .then(blob => {
                            downloadButton.onclick = function(event){
                                let url = URL.createObjectURL(blob);
                                let a = document.createElement('a');
                                a.style.display = 'none';
                                a.href = url;
                                a.download = filename;
                                document.body.appendChild(a);
                                a.click();
                                window.URL.revokeObjectURL(url);
                                a.remove();
                            }  
                            downloadButtonDiv.style.display = "block";
                            
                            uploadFile(blob, filename);                          
                        })
                };
            };
            reader.readAsDataURL(input.files[0]);
        }
    }
    function uploadFile(blob, filename='image.png') {
        var formData = new FormData();
        formData.append('file', blob, filename);
        var ajax = new XMLHttpRequest();
        ajax.onload = () => {
            let res = JSON.parse(ajax.responseText);
            let resUrl = res.file.url;
            
            document.getElementById("result").innerHTML=`Shareable Link: <a href="${resUrl}">${resUrl}</a>`; 
        };
        ajax.open('POST', '/upload');
        ajax.send(formData);
    }
</script>
</html>